<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ac.ict.mapper.TtSosBindMapper">

    <select id="getMacBind" resultType="java.lang.String">
        SELECT binds.mac
        FROM tt_sos_bind binds
        WHERE binds.bind_status=#{bindStatus}
    </select>

</mapper>